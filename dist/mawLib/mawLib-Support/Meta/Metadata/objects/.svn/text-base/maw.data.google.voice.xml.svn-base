<!-- REQUIRED: object root, object group, type:(external, mxj, js, abstraction), tags:(comma separated), library: website -->
<object name="maw.data.google.voice" 
		group="data"
		type="mxj" 
		tags="help,template"
		url="http://dev.minneapolisartonwheels.org/mawLib/maw.data.google.voice"> <!-- documentation page -->
	
	<!-- REQUIRED: a single line description -->	
	<digest name="An mxj external to interact with Google Voice."/>

	<!-- REQUIRED: a short summary of the function -->	
	<summary name="This object can connect to Google Voice, download messages, send and receive SMS, transcribed messages, etc."/>

	<!-- REQUIRED: a bit of license text -->
